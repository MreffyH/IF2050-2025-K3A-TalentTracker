         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" layoutX="226.0" layoutY="134.0" promptText="Username" />
      <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="188.0" promptText="Password" />
      <Button fx:id="loginButton" layoutX="274.0" layoutY="250.0" mnemonicParsing="false" text="Login" />
      <Label fx:id="statusLabel" layoutX="226.0" layoutY="310.0" prefHeight="17.0" prefWidth="149.0" textFill="RED" />
      <Button fx:id="registerButton" layoutX="450.0" layoutY="350.0" mnemonicParsing="false" text="Register" />
   </children>
</AnchorPane> 